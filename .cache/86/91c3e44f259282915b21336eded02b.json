 font-weight: 500;\n  background-color: #268dd1;\n  background-size: cover;\n}\n\n.container {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: center;\n  padding-right: 25px;\n  padding-left: 25px;\n}\n\n.search {\n  display: flex;\n  justify-content: center;\n  margin-top: 100px;\n  margin-bottom: 25px;\n}\n\n.input {\n  width: 500px;\n  border: 1px solid rgb(255, 255, 255);\n  box-sizing: border-box;\n  filter: drop-shadow(0px 4px 4px rgba(0, 0, 0, 0.15));\n  margin-right: 25px;\n  border-radius: 10px;\n  background-color: inherit;\n  padding: 15px;\n  height: 80px;\n  font-size: 20px;\n}\n\n.input:focus {\n  outline-color: $accent-color;\n}\n\n.input::placeholder {\n  color: $input-color;\n}\n\n.btn {\n  min-width: 200px;\n  height: 80px;\n  background-color: $accent-color;\n  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.15);\n  border: none;\n  border-radius: 5px;\n  font-weight: 700;\n  font-size: 30px;\n  line-height: 1.87;\n  text-align: center;\n  letter-spacing: 0.03em;\n  color: $btn-color;\n}\n\n.btn:hover,\n.btn:focus {\n  background-color: $btn-hover-bgcolor;\n  cursor: pointer;\n  outline-color: $accent-color;\n}\n\n.load-more {\n  margin-top: 20px;\n  margin-bottom: 40px;\n}\n\n.gallery {\n  display: flex;\n  flex-wrap: wrap;\n  align-items: center;\n  justify-content: center;\n}\n\n.photo-card {\n  width: 100%;\n  overflow: hidden;\n\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n\n  background-color: #fff;\n  box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.12), 0px 4px 4px rgba(0, 0, 0, 0.06),\n    1px 4px 6px rgba(0, 0, 0, 0.16);\n}\n\n.photo-card__item {\n  display: flex;\n  flex-direction: column;\n\n  margin: 15px;\n  width: calc((100% - 8 * 15px) / 4);\n  border-radius: 10px;\n  overflow: hidden;\n}\n.photo-image__wrapper {\n  overflow: hidden;\n}\n\n.photo-image {\n  display: block;\n  height: 100%;\n  width: 100%;\n  object-fit: cover;\n  height: 15vw;\n  transition: transform linear 250ms;\n}\n\n.photo-image:hover {\n  cursor: zoom-in;\n  transform: scale(1.1);\n}\n\n.stats {\n  display: flex;\n  justify-content: center;\n  margin-top: 15px;\n}\n\n.stats-item {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  font-weight: 400;\n  font-size: 20px;\n  margin-bottom: 10px;\n  color: rgb(0, 0, 0);\n}\n\n.stats-item:not(:last-child) {\n  margin-right: 25px;\n}\n\n.material-icons {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 10px;\n  color: $accent-color;\n}"},"lineCount":null}},"error":null,"hash":"3948a562751d8d1ba8bfbb569b242f43","cacheData":{"env":{}}}